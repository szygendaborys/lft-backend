{
  "name": "lft-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:unit": "jest --testPathIgnorePatterns=\"int\\.spec\\.ts\" --forceExit",
    "test:int": "jest --testPathPattern=.int.spec\\.ts$ --runInBand --forceExit",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@nestjs/common": "^7.5.1",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.5.1",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.18",
    "@nestjs/swagger": "^4.8.0",
    "@nestjs/typeorm": "^7.1.5",
    "@sendgrid/mail": "^7.6.0",
    "async-local-storage": "^2.3.1",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.2",
    "helmet": "^5.1.1",
    "joi": "^17.6.0",
    "jwt-decode": "^3.1.2",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "nest-router": "^1.0.9",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "request-context": "^2.0.0",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.34",
    "typeorm-transactional-cls-hooked": "^0.1.21",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^7.5.1",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.2",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.182",
    "@types/node": "^14.14.6",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "^3.4.1",
    "faker": "^5.5.3",
    "husky": "^8.0.1",
    "jest": "^26.6.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "!<rootDir>/migrations/*",
      "!<rootDir>/src/main.ts",
      "!<rootDir>/src/setup.swagger.ts",
      "!<rootDir>/src/v2/domain/events/*",
      "!<rootDir>/test/*",
      "!<rootDir>/src/**/*dto.ts",
      "!<rootDir>/src/**/*exception.ts"
    ],
    "setupFilesAfterEnv": [
      "../jest.setup.ts"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
