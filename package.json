{
  "name": "lft-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:unit": "jest --testPathIgnorePatterns=\"int\\.spec\\.ts\" --forceExit",
    "test:int": "jest --testPathPattern=.int.spec\\.ts$ --runInBand --forceExit",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:up": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run -d datasource.ts",
    "migration:down": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:revert -d datasource.ts"
  },
  "dependencies": {
    "@nestjs/axios": "^0.1.0",
    "@nestjs/cli": "^9.1.5",
    "@nestjs/common": "^9.1.6",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.6",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/platform-socket.io": "^9.2.1",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.2.1",
    "@sendgrid/mail": "^7.7.0",
    "@sentry/node": "^7.24.2",
    "axios": "^1.2.4",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "helmet": "^6.0.1",
    "joi": "^17.7.0",
    "jwt-decode": "^3.1.2",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "nest-router": "^1.0.9",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7",
    "swagger-ui-express": "^4.6.0",
    "typeorm": "^0.3.11",
    "typeorm-transactional-cls-hooked": "^0.1.21",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.1",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.15",
    "@types/faker": "^5.5.9",
    "@types/jest": "^28.1.8",
    "@types/lodash": "^4.14.191",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.18",
    "@types/passport-jwt": "^3.0.7",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "faker": "^5.5.3",
    "husky": "^8.0.3",
    "jest": "^28.1.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "supertest": "^6.3.3",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "!<rootDir>/migrations/*",
      "!<rootDir>/src/main.ts",
      "!<rootDir>/src/setup.swagger.ts",
      "!<rootDir>/src/v2/domain/events/*",
      "!<rootDir>/test/*",
      "!<rootDir>/src/**/*dto.ts",
      "!<rootDir>/src/**/*exception.ts"
    ],
    "setupFilesAfterEnv": [
      "../jest.setup.ts"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
